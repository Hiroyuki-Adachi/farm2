document.getElementById("tab").innerHTML = "<%= escape_javascript(render(partial: "tab1")) %>";
new Chart(document.getElementById("chart").getContext('2d'), {
    type: 'bar',
    data: {
        labels: <%= @total_all.map {|ta| ta[0] }.to_json.html_safe %>,
        datasets: [{
            label: "作業時間",
            data: <%= @total_all.map {|ta| ta[1].to_f } %>,
            backgroundColor: colors[0],
            fill: false
        }]
    }
});
