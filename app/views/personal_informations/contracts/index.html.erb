<% if @works.count > 0 %>
  <table id="personal_table" class="type06 table table-sm">
    <tbody>
      <% @works.each do |work| %>
        <tr>
          <td><%= work_type_icon_tag(work.work_type) %></td>
          <td>
            <div><%= work.worked_at %></div>
            <div class="small"><%= work.model.start_at.strftime("%H:%M") %>ã€œ<%= work.model.end_at.strftime("%H:%M") %></div>
          </td>
          <td>
            <div><%= work.work_kind.name %></div>
            <div class="small"><%= work.name %></div>
          </td>
          <td style="text-align:right;"><%= work.sum_hours %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
