<h1 class="page-header">作業計画入力</h1>
<%= error_print(@schedule) %>

<%= form_for(@schedule.model, url: {action: :create }, html: {method: :post}) do |f| %>
  <%= render(partial: 'form', locals: {:f => f}) %>
  <div class="btn-toolbar">
    <div class="btn-group mr-2">
      <%= f.submit '登録', name: 'regist', class: "btn btn-warning" %>
    </div>
    <div class="btn-group mr-2">
      <%= button_tag "戻る",  type: :button, onclick: raw("javascript:location.href = '" + schedules_path + "';"), class: "btn btn-outline-dark" %>
    </div>
  </div>
<% end %>
