<div class="modal fade" id="popup_alert" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-body">
        <label id="popup_alert_message" class="form-label"></label>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">閉じる</button>
      </div>
    </div>
  </div>
</div>
<div class="modal fade" id="popup_confirm" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-body">
        <label id="popup_confirm_message" class="form-label"></label>
      </div>
      <div class="modal-footer">
        <button type="button" id="popup_confirm_yes" class="btn btn-primary confirm-button" value="true">はい</button>
        <button type="button" id="popup_confirm_no" class="btn btn-secondary confirm-button" value="false">いいえ</button>
      </div>
    </div>
  </div>
</div>
<dialog id="turbo_confirm">
  <form method="dialog">
    <p>よろしいですか?</p>
    <div>
      <button value="confirm">はい</button>
      <button value="cancel">いいえ</button>
    </div>
  </form>
</dialog>
